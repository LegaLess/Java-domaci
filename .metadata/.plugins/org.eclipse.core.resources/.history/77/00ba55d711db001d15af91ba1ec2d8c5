package skijanje;

public abstract class Deonica {
	
	private float duzina;
	private float nagib;
	protected char oznaka;
	
	public abstract char oznaka();
	public abstract double ubrzanje();
	
	public Deonica(double duzina, double nagib) {
		this.duzina = duzina;
		this.nagib = nagib;
	}
	
	public double duzina() {
		return duzina;
	}
	
	public double nagib() {
		return nagib;
	}
	
	public float brzina(float pocetnaBrzina) {
		return Math.sqrt(2 * this.ubrzanje());
	}
	
	
}
